 <section id="contact" class="contact section">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>Get In Touch</h2>
        <p>I'm always open to new opportunities and exciting projects. Feel free to contact me using the information below.</p>
      </div><!-- End Section Title -->

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4">

          <div class="col-lg-5">

            <div class="info-wrap">
              <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="200">
                <i class="bi bi-geo-alt flex-shrink-0"></i>
                <div>
                  <h3>Address</h3>
                  <p><%= setting.contactSection.address %></p>
                </div>
              </div><!-- End Info Item -->

              <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="300">
                <i class="bi bi-telephone flex-shrink-0"></i>
                <div>
                  <h3>Call Us</h3>
                  <p><%= setting.contactSection.phone %></p>
                </div>
              </div><!-- End Info Item -->

              <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="400">
                <i class="bi bi-envelope flex-shrink-0"></i>
                <div>
                  <h3>Email Us</h3>
                  <p><%= setting.contactSection.email %></p>
                </div>
              </div><!-- End Info Item --> 

              <iframe src="<%= setting.contactSection.mapEmbedUrl %>" frameborder="0" style="border:0; width: 100%; height: 270px;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
          </div>

          <div class="col-lg-7">
            
      
          <!-- Contact Form -->

   <form action="/contact" method="POST" class="needs-validation" novalidate>
  <div class="row gy-4">

    <div class="col-md-6">
      <label for="name-field" class="form-label">Your Name</label>
      <input type="text" name="name" id="name-field" class="form-control" required>
    </div>

    <div class="col-md-6">
      <label for="email-field" class="form-label">Your Email</label>
      <input type="email" name="email" id="email-field" class="form-control" required>
    </div>

    <div class="col-md-12">
      <label for="subject-field" class="form-label">Subject</label>
      <input type="text" name="subject" id="subject-field" class="form-control" required>
    </div>

    <div class="col-md-12">
      <label for="message-field" class="form-label">Message</label>
      <textarea name="message" id="message-field" rows="6" class="form-control" required></textarea>
    </div>

    <div class="col-md-12 text-center mt-3">
      <button type="submit" class="btn btn-primary">Send Message</button>
    </div>

  </div>
</form>

          </div><!-- End Contact Form -->

        </div>

      </div>

    </section>